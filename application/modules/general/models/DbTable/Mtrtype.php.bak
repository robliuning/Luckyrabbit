class General_Models_DbTable_Mtrtype extends Zend_Db_Table_Abstract
{
    protected $_name = 'ge_mtrtypes';

	public function findTypeName($id)
    {
    	$select = $this->select()
			->setIntegrityCheck(false)
			->from('ge_mtrtypes',array('name'))
			->where('contactId = ?',$id);
			
   		$entries = $this->fetchAll($select);
   		
   		return $entries;
    	}
}

